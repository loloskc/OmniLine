@using OmniLine.ViewModel;
@model CreateAgentVM

<form method="post" asp-action="Edit" enctype="multipart/form-data">
    <div class="form-group">
        <label>Название контрагента</label>
        <input asp-for="Agent.Name" class="form-control" placeholder="Название контрагента">
        <span class="text-danger"></span>
    </div>

    @if (true)
    {
        <div class="form-group">
            <label>Дата Создани</label>
            <input asp-for="Agent.DateCreate" class="form-control" placeholder="dateCreate" readonly>
            <span class="text-danger"></span>
        </div>
    }
    <hr />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>